{
  "type": "module",
  "private": true,
  "scripts": {
    "build:prod": "webpack --env mode='production'",
    "build:dev": "webpack --env mode='development'",
    "start": "webpack serve --env port=3000 --env mode='development'",
    "lint:ts": "eslint --ext .ts --ext .tsx ./",
    "lint:ts:fix": "eslint --ext .ts --ext .tsx ./ --fix",
    "lint:ts:old": "eslint \"**/*.{ts, tsx}\"",
    "lint:ts:fix:old": "eslint \"**/*.{ts, tsx}\" --fix",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "storybook": "start-storybook -p 6006 -c ./config/storybook -s ./public",
    "storybook:build": "build-storybook -c ./config/storybook -s ./public"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@library/build": "file:./frontend-react-library",
    "@storybook/addon-actions": "^8.5.1",
    "@storybook/addon-essentials": "^8.5.1",
    "@storybook/addon-interactions": "^8.5.1",
    "@storybook/addon-links": "^8.5.1",
    "@storybook/builder-webpack5": "^8.5.1",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^8.5.1",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/webpack": "^5.28.5",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "sass": "^1.83.4",
    "stylelint": "^16.13.2",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-semantic-groups": "^1.2.1",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "resolutions": {
    "@library/build": "portal:frontend-react-library/packages/build"
  },
  "workspaces": [
    "frontend-react-library"
  ]
}
